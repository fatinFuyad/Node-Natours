{
  "extends": [
    // "airbnb",
    // "prettier",
    "plugin:node/recommended"
  ],
  // "plugins": [
  //   "prettier"
  // ],
  "rules": {
    // "prettier/prettier": [
    //   "off",
    //   {
    //     "endOfLine": "auto"
    //   }
    // ],
    "quotes": "off",
    "spaced-comment": "off",
    "no-console": "warn",
    "consistent-return": "off",
    "func-names": "off",
    "object-shorthand": "off",
    "no-process-exit": "off",
    "no-param-reassign": "off",
    "no-return-await": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "prefer-object-spread": "off",
    "radix": "off",
    "comma-dangle": "off",
    "prefer-destructuring": [
      "error",
      {
        "object": true,
        "array": false
      }
    ],
    "no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "req|res|next|val"
      }
    ],
    "prefer-template": "off"
  }
}